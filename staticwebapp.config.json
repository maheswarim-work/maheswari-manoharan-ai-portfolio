{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "routes": [
    {
      "route": "/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=3600"
      }
    }
  ],
  "mimeTypes": {
    ".json": "application/json"
  }
}